{
  "name": "@blazzi/eslint-plugin",
  "version": "0.1.0",
  "description": "Blazzi ESLint plugin",
  "keywords": [
    "plugin",
    "eslint",
    "lint"
  ],
  "author": "Mehmetcan YILMAZ",
  "license": "MIT",
  "type": "module",
  "homepage": "https://github.com/ymehmetcan/blazzi-toolkit/tree/main/packages/eslint-plugin#readme",
  "bugs": {
    "url": "https://github.com/ymehmetcan/blazzi-toolkit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ymehmetcan/blazzi-toolkit.git",
    "directory": "packages/eslint-plugin"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint:types": "tsc --noEmit"
  },
  "dependencies": {
    "@typescript-eslint/utils": "8.13.0"
  },
  "devDependencies": {
    "@blazzi/ts-config": "workspace:*",
    "tsup": "8.3.5",
    "typescript": "5.6.3"
  }
}
