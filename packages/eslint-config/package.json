{
  "name": "@blazzi/eslint-config",
  "version": "0.0.2",
  "description": "Blazzi ESLint config",
  "keywords": [
    "config",
    "eslint",
    "lint"
  ],
  "author": "Mehmetcan YILMAZ",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ymehmetcan/blazzi-toolkit.git",
    "directory": "packages/eslint-config"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./base": "./src/configs/base.js",
    "./prettier": "./src/configs/prettier.js",
    "./node": "./src/configs/node.js",
    "./typescript": "./src/configs/typescript.js",
    "./react": "./src/configs/react.js",
    "./next": "./src/configs/next.js"
  },
  "files": [
    "*.md",
    "LICENSE",
    "src"
  ],
  "scripts": {
    "lint": "eslint . --max-warnings 0 --ext .js,.cjs,.mjs",
    "lint:fix": "eslint . --ext .js,.cjs,.mjs --fix"
  },
  "dependencies": {
    "@blazzi/eslint-plugin": "workspace:*",
    "@rushstack/eslint-patch": "1.6.1",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0"
  },
  "peerDependencies": {
    "eslint": "^8.3.0"
  }
}
