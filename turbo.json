{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "topo": {
      "dependsOn": ["^topo"]
    },
    "//#lint": {
      "dependsOn": []
    },
    "//#format:check": {
      "dependsOn": []
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "typecheck": {
      "dependsOn": ["topo"]
    }
  }
}
